{
  "name": "@rivetkit/next-js",
  "version": "2.0.3",
  "description": "Next.js integration for RivetKit actors and client",
  "license": "Apache-2.0",
  "keywords": [
    "rivetkit",
    "nextjs",
    "next",
    "nextjs-framework",
    "next-js",
    "react",
    "framework",
    "web",
    "serverless",
    "edge",
    "actor",
    "microservices"
  ],
  "sideEffects": false,
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.mts",
        "default": "./dist/mod.mjs"
      },
      "require": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/mod.d.mts",
        "default": "./dist/client/mod.mjs"
      },
      "require": {
        "types": "./dist/client/mod.d.ts",
        "default": "./dist/client/mod.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/mod.ts src/client/mod.ts",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@rivetkit/react": "workspace:*",
    "rivetkit": "workspace:^",
    "hono": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2"
  },
  "stableVersion": "0.8.0"
}
