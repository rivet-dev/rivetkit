{
  "name": "actor-core",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src",
    "deno.json",
    "bun.json",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/mod.d.ts",
        "default": "./dist/client/mod.js"
