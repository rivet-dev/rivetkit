{
  "name": "@rivetkit/react",
  "version": "0.9.1",
  "license": "Apache-2.0",
  "keywords": [
    "rivetkit",
    "react",
    "hooks",
    "components",
    "frontend",
    "ui"
  ],
  "sideEffects": false,
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.mts",
        "default": "./dist/mod.mjs"
      },
      "require": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/mod.tsx",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@rivetkit/framework-base": "workspace:*",
    "@tanstack/react-store": "^0.7.1",
    "@rivetkit/core": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2"
  },
  "stableVersion": "0.8.0"
}
