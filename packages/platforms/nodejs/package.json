{
  "name": "@rivetkit/nodejs",
  "version": "0.9.0-rc.1",
  "keywords": [
    "rivetkit",
    "nodejs",
    "platform",
    "server",
    "runtime"
  ],
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/mod.ts",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "@rivetkit/file-system": "*",
    "@rivetkit/memory": "*",
    "rivetkit": "*"
  },
  "devDependencies": {
    "@rivetkit/file-system": "workspace:*",
    "@rivetkit/memory": "workspace:*",
    "hono": "^4.8.0",
    "rivetkit": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.4",
    "@hono/node-ws": "^1.0.8",
    "@types/node": "^24.0.3",
    "zod": "^3.24.2"
  },
  "stableVersion": "0.8.0"
}
