{
  "name": "@rivetkit/rivet",
  "version": "0.9.0-rc.1",
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    },
    "./worker": {
      "import": {
        "types": "./dist/worker.d.ts",
        "default": "./dist/worker.js"
      },
      "require": {
        "types": "./dist/worker.d.cts",
        "default": "./dist/worker.cjs"
      }
    },
    "./manager": {
      "import": {
        "types": "./dist/manager.d.ts",
        "default": "./dist/manager.js"
      },
      "require": {
        "types": "./dist/manager.d.cts",
        "default": "./dist/manager.cjs"
      }
    },
    "./tsconfig": "./dist/tsconfig.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/mod.ts src/worker.ts src/manager.ts",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "peerDependencies": {
    "rivetkit": "*"
  },
  "devDependencies": {
    "@rivet-gg/actor-core": "^25.1.0",
    "@rivet-gg/api": "^25.4.2",
    "@types/deno": "^2.0.0",
    "@types/invariant": "^2",
    "@types/node": "^22.13.1",
    "rivetkit": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.4",
    "@hono/node-ws": "^1.1.7",
    "hono": "^4.7.0",
    "invariant": "^2.2.4",
    "zod": "^3.24.2"
  },
  "stableVersion": "0.8.0"
}
