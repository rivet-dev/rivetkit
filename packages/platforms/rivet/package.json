{
  "name": "@actor-core/rivet",
  "version": "0.2.0",
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "deno": "./src/mod.ts",
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/mod.ts",
    "dev": "yarn build --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "actor-core": "workspace:*"
  },
  "devDependencies": {
    "@rivet-gg/actor-core": "^25.1.0",
    "@types/deno": "^2.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "hono": "^4.6.20"
  }
}
